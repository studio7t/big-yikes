{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "package": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "out/**"
      ]
    },
    "deploy": {
      "dependsOn": [
        "package"
      ],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "lint": {}
  }
}
