{
  "name": "@big-yikes/server",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "esbuild --bundle --minify --outfile=dist/handler.js --platform=node --sourcemap --external:pg-native --define:process.env.NODE_ENV=\\\"production\\\" --tree-shaking=true src/handler.ts",
    "package": "mkdir -p out && zip -j out/server.zip dist/handler.js",
    "dev": "ts-node-dev src/index.ts",
    "start": "node ./dist/index.js",
    "test": "jest --runInBand",
    "lint": "eslint ./src && prettier --check ./src"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.218.0",
    "@aws-sdk/lib-dynamodb": "^3.218.0",
    "@big-yikes/lib": "*",
    "@fastify/cors": "^8.1.0",
    "@fastify/mongodb": "^6.0.1",
    "@fastify/type-provider-typebox": "^2.3.0",
    "@sinclair/typebox": "^0.24.28",
    "dotenv": "^16.0.1",
    "fastify": "^4.0.2",
    "fastify-auth0-verify": "^0.8.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "pg": "^8.8.0",
    "postgres-migrations": "^5.3.0",
    "serverless-http": "^3.1.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "aws-cdk-lib": "^2.52.0",
    "constructs": "^10.1.173",
    "eslint": "^8.17.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  }
}
